{
  "name": "healthcheck-dashboard",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:4001",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server-start": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server-start\" \"npm run start\"",
    "test:securityallWizard": "snyk wizard",
    "test:securityWizard": "snyk wizard --severity-threshold=medium",
    "test:security": "snyk auth && snyk test --severity-threshold=medium"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "emotion": "9.2.12",
    "govuk-react": "0.3.1",
    "jsonwebtoken": "^8.4.0",
    "mongodb": "^3.1.13",
    "node-fetch": "^2.2.0",
    "pg": "^7.8.0",
    "react": "^16.5.1",
    "react-cookie": "^3.0.8",
    "react-dom": "^16.5.1",
    "react-emotion": "9.2.8",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "sequelize": "^6.0.0",
    "snyk": "^1.134.0"
  },
  "devDependencies": {
    "react-markdown": "3.3.2"
  },
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
